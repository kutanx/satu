def publik(romz):
	try:
		os.mkdir('dump')
	except:pass
	try:
		print ("\n%s [%s!%s] INGAT '%sID%s' HARUS BERSIFAT PUBLIK "%(P,M,P,H,P))
		print ("%s [%s!%s] Ketik '%sme%s' jika ingin dump daftar teman sendiri "%(P,M,P,H,P))
		idt = raw_input(" %s[%s!%s] masukan id publik : "%(P,U,P))
		x = requests.get('https://graph.facebook.com/%s?fields=name,friends.fields(id,name).limit(5000)&access_token=%s'%(idt,romz))
		z = json.loads(x.text)
		file = ('dump/'+z['name']+'.json').replace(' ', '_')
		bff = open(file, 'w')
		for a in z['friends']['data']:
			id.append(a["id"]+"<=>"+a["name"])
			bff.write(a["id"]+"<=>"+a["name"]+ "\n")
			sys.stdout.write("\r %s[%s*%s] sedang mengumpulkan id... [%s%s%s%s]"%(P,U,P,ior,H,str(len(id)),P))
			sys.stdout.flush()
			time.sleep(0.005)
		bff.close()
		print ('\n%s [%s‚àö%s] File dump tersimpan :%s %s '%(P,H,P,H,file))
		raw_input('\n%s [ %senter %s] '%(P,K,P))
		menu()
	except IOError:
		print(" %s[%s√ó%s] gagal membuat file"%(P,M,P))
		exit()
	except (KeyboardInterrupt, EOFError):
		print(" %s[%s√ó%s] proses mengambil id terhenti"%(P,M,P))
		exit()
	except KeyError:
		print(" %s[%s√ó%s] gagal mengambil id kemungkin token invalid atau id tidak publik"%(P,M,P))
		exit()
	except OSError:
		print(" %s[%s√ó%s] file tidak tersimpan"%(P,M,P))
		exit()
	except requests.exceptions.ConnectionError:
		print(" %s[%s√ó%s] tidak ada koneksi internet"%(P,M,P))
        

